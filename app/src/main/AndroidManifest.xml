<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.farmerproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".MyActivities.Chats"></activity>
        <activity android:name=".MyActivities.Complaint_status" />
        <activity android:name=".MyActivities.SupplierLogin" />
        <activity android:name=".MyActivities.Test" />
        <activity android:name=".MyActivities.Farmshop" />
        <activity android:name=".MyActivities.complaint" />

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />

        <activity android:name=".MyActivities.FarmerMainPage" />
        <activity android:name=".MyActivities.FarmerRealLog" />
        <activity android:name=".Commons.DatabaseHelper" />
        <activity android:name=".MyActivities.FarmerLogin" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>